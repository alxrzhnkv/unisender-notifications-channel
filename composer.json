{
    "name": "tzurbaev/unisender-notifications-channel",
    "description": "Unisender Notifications Channel for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.6.*",
        "illuminate/notifications": "5.6.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Unisender\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Unisender\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Unisender\\Providers\\UnisenderChannelServiceProvider"
            ]
        }
    }
}
